  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri_prod",
    "NEXTAUTH_SECRET": "@nextauth_secret_prod",
